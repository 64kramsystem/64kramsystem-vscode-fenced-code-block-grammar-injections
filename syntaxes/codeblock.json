{
	"fileTypes": [],
	"injectionSelector": "L:markup.fenced_code.block.markdown",
	"patterns": [
		{
			"include": "#superjs-code-block"
		}
	],
	"repository": {
		"superjs-code-block": {
			"begin": "superjs(\\s+[^`~]*)?$",
			"end": "(^|\\G)(?=\\s*[`~]{3,}\\s*$)",
			"contentName": "meta.embedded.block.superjs",
			"patterns": [
				{
					"include": "source.js"
				}
			]
		}
	},
	"scopeName": "markdown.superjs.codeblock"
}