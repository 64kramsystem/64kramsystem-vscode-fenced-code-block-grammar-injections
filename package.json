  "license": "MIT",
  "repository": {
    "url": "https://github.com/64kramsystem/vscode-fenced-code-block-grammar-injections.git"
  },
  "bugs": {
    "url": "https://github.com/64kramsystem/vscode-fenced-code-block-grammar-injections/issues"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "superasm-injection"
      }
    ],
    "grammars": [
      {
        "language": "superasm-injection",
        "scopeName": "markdown.asm.codeblock",
        "path": "./syntaxes/codeblock_asm.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.asm": "asm"
        }
      }
    ]
  },
  "dependencies": {}
}